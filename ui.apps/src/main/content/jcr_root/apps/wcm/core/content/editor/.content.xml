<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:rep="internal"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="nt:unstructured">
    <jcr:content jcr:primaryType="nt:unstructured">
        <sidepanels jcr:primaryType="nt:unstructured">
            <edit jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <tabs jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <cgOTMMPlugin
                                    granite:class="sidepanel-tab sidepanel-tab-components"
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="CG OTMM Plugin"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <granite:data
                                        jcr:primaryType="nt:unstructured"
                                        headerTitle="CG OTMM Plugin"/>
                                <parentConfig
                                        jcr:primaryType="nt:unstructured"
                                        icon="pageTag"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                            granite:class="sidepanel-tab-title"
                                            granite:id="tag-title"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/heading"
                                            text="CG OTMM Plugin"/>
                                    <otmm-main granite:class="sidepanel-tab-title" jcr:primaryType="nt:unstructured"
                                               sling:resourceType="aemconnector-new/components/cgotmmwidgetnew"
                                               text="OTMM Plugin Manager New"/>
                                </items>
                            </cgOTMMPlugin>
                        </items>
                    </tabs>
                </items>
            </edit>
        </sidepanels>
    </jcr:content>
</jcr:root>
